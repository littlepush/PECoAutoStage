.func do_in_loop
$.void = $.stdout($.loop_index)

.stage
$.loop_index = 0
$.void = $.loop($.expr({
    $.return = $.loop_index < 10
}), $.job({
    $.void = $.invoke("do_in_loop")
}), $.after({
    $.loop_index = $.loop_index + 1
}))

$.github = $.httpreq("https://github.com")
$.void = $.stdout($.github)
$.github_result = $.github.send()
$.void = $.stdout($.github_result)
